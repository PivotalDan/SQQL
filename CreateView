CREATE VIEW `AccountswithCustom` AS select `a`.`id` AS `id`,`a`.`name` AS `name`,`a`.`date_entered` AS `date_entered`,`a`.`date_modified` AS `date_modified`,`a`.`modified_user_id` AS `modified_user_id`,`a`.`created_by` AS `created_by`,`a`.`description` AS `description`,`a`.`deleted` AS `deleted`,`a`.`assigned_user_id` AS `assigned_user_id`,`a`.`account_type` AS `account_type`,`a`.`industry` AS `industry`,`a`.`annual_revenue` AS `annual_revenue`,`a`.`phone_fax` AS `phone_fax`,`a`.`billing_address_street` AS `billing_address_street`,`a`.`billing_address_city` AS `billing_address_city`,`a`.`billing_address_state` AS `billing_address_state`,`a`.`billing_address_postalcode` AS `billing_address_postalcode`,`a`.`billing_address_country` AS `billing_address_country`,`a`.`rating` AS `rating`,`a`.`phone_office` AS `phone_office`,`a`.`phone_alternate` AS `phone_alternate`,`a`.`website` AS `website`,`a`.`ownership` AS `ownership`,`a`.`employees` AS `employees`,`a`.`ticker_symbol` AS `ticker_symbol`,`a`.`shipping_address_street` AS `shipping_address_street`,`a`.`shipping_address_city` AS `shipping_address_city`,`a`.`shipping_address_state` AS `shipping_address_state`,`a`.`shipping_address_postalcode` AS `shipping_address_postalcode`,`a`.`shipping_address_country` AS `shipping_address_country`,`a`.`parent_id` AS `parent_id`,`a`.`sic_code` AS `sic_code`,`a`.`campaign_id` AS `campaign_id`,`b`.`id_c` AS `id_c`,`b`.`strat1_c` AS `strat1_c`,`b`.`strat2_c` AS `strat2_c`,`b`.`strat3_c` AS `strat3_c`,`b`.`pcfplay1_c` AS `pcfplay1_c`,`b`.`pcfplay2_c` AS `pcfplay2_c`,`b`.`pcfplay3_c` AS `pcfplay3_c`,`b`.`bdsplay1_c` AS `bdsplay1_c`,`b`.`bdsplay2_c` AS `bdsplay2_c`,`b`.`labsinnovation_c` AS `labsinnovation_c`,`b`.`adoption_rate_c` AS `adoption_rate_c`,`b`.`bds_play2_c` AS `bds_play2_c`,`b`.`datascience_c` AS `datascience_c`,`b`.`acctrep_c` AS `acctrep_c`,`b`.`customerdataapps_c` AS `customerdataapps_c`,`b`.`pastvaluedelivered_c` AS `pastvaluedelivered_c`,`b`.`customerbusinesschg_c` AS `customerbusinesschg_c`,`b`.`whoimportantto_c` AS `whoimportantto_c`,`b`.`important_to_whom_c` AS `important_to_whom_c`,`b`.`how_to_create_value_c` AS `how_to_create_value_c`,`b`.`aws_c` AS `aws_c`,`b`.`azure_c` AS `azure_c`,`b`.`google_c` AS `google_c`,`b`.`softlayer_c` AS `softlayer_c`,`b`.`vmware_c` AS `vmware_c`,`b`.`openstack_c` AS `openstack_c`,`b`.`docker_c` AS `docker_c`,`b`.`mesosphere_c` AS `mesosphere_c`,`b`.`openshift_c` AS `openshift_c`,`b`.`bluemix_c` AS `bluemix_c`,`b`.`pivotalcf_c` AS `pivotalcf_c`,`b`.`kubernetes_c` AS `kubernetes_c`,`b`.`diypaas_c` AS `diypaas_c`,`b`.`primary_pa_c` AS `primary_pa_c`,`b`.`sfdc_c` AS `sfdc_c`,`b`.`legacy_replatform_wip_c` AS `legacy_replatform_wip_c`,`b`.`legacy_replatform_details_c` AS `legacy_replatform_details_c`,`b`.`security_play_wip_c` AS `security_play_wip_c`,`b`.`security_play_details_c` AS `security_play_details_c`,`b`.`next_gen_appdev_wip_c` AS `next_gen_appdev_wip_c`,`b`.`next_gen_appdev_details_c` AS `next_gen_appdev_details_c`,`b`.`devops_automation_c` AS `devops_automation_c`  from (`accounts` `a` join `accounts_cstm` `b` on((`a`.`id` = `b`.`id_c`)));
